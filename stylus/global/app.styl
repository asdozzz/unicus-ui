.unicus-head
  height: 77px;
  background-color: $colors.head!important;
  box-shadow: 0 2px 11px 0 rgba(0, 0, 0, 0.22);

.unicus-badge
  font-family: $fonts.muzeo-500;
  font-size: $font-sizes.badge;
  background-color: $colors.badge;
  background-image: linear-gradient(83deg, rgba(2, 165, 170, 0.99), #04c4a6);
  border-radius(23px);
  color:#f7f7f7;
  padding:0 10px;

.unicus-time
  font-family: $fonts.open-sans;
  font-size: $font-sizes.badge;
  color:$colors.label;
  padding:0 5px;

